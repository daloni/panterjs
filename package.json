{
  "name": "panterjs",
  "version": "1.0.0",
  "private": true,
  "description": "A simple boilerplate for web application.",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "start-dev": "DEBUG=panterjs:* nodemon server.js",
    "db:migrate": "knex migrate:latest --knexfile=database/knexfile.js",
    "db:seed": "knex seed:run --knexfile=database/knexfile.js",
    "knex": "knex --knexfile=database/knexfile.js",
    "lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ffiguereo/panterjs.git"
  },
  "author": "Francisco Figuereo <ffiguereo2@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ffiguereo/panterjs/issues"
  },
  "homepage": "https://github.com/ffiguereo/panterjs#readme",
  "devDependencies": {
    "nodemon": "1.15.1",
    "standard": "11.0.0"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.18.2",
    "chalk": "2.3.1",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "debug": "3.1.0",
    "dotenv": "5.0.0",
    "express": "4.16.2",
    "express-session": "1.15.6",
    "faker": "^4.1.0",
    "knex": "0.14.4",
    "method-override": "2.3.10",
    "morgan": "1.9.0",
    "objection": "1.0.0",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "pg": "7.4.1",
    "pg-hstore": "2.3.2",
    "pug": "2.0.0-rc.4",
    "rotating-file-stream": "1.3.4"
  }
}
